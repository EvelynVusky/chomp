
void foo (int a) {
    if (a > 4) {
        println("greater");
        if (a == 5) {
            println("greater yes");
        } else {
            println("greater no");
        }
    } else {
        println("less");
        if (a == 3) {
            println("less yes");
        } else {
            println("less no");
        }
    }

}

int main () {
    foo(5);
    foo(6);
    foo(3);
    foo(2);

    int a = 7;

    if (a > 4) {
        println("greater");
        if (a == 5) {
            println("greater yes");
        } else {
            println("greater no");
        }
    } else {
        println("less");
        if (a == 3) {
            println("less yes");
        } else {
            println("less no");
        }
    }
    return 0;
}